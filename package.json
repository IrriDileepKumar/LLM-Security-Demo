{
  "name": "vulnerable-llms",
  "version": "1.0.0",
  "description": "OWASP Top 10 for LLMs interactive vulnerability demonstrations",
  "main": "frontend/dist/index.js",
  "private": true,
  "scripts": {
    "build": "cd frontend && npm run build",
    "build:server": "cd frontend && npm run build:server",
    "dev": "cd frontend && npm run dev",
    "dev:http": "cd frontend && npm run dev:http",
    "dev:watch": "cd frontend && npm run dev:watch",
    "server:dev": "cd frontend && npm run server:dev",
    "server:dev:http": "cd frontend && npm run server:dev:http",
    "start": "cd frontend && node dist/index.js",
    "start:http": "cd frontend && node dist/index.js http 3000",
    "install:backend": "pip install --upgrade pip && pip install -r requirements.txt",
    "install:frontend": "cd frontend && npm install",
    "build:docker": "docker-compose build",
    "dev:docker": "docker-compose -f docker-compose.override.yml up --build",
    "test": "cd frontend && vitest",
    "test:frontend": "cd frontend && npm run test:run",
    "test:backend": "pytest",
    "test:all": "npm run test:backend && npm run lint && npm run typecheck && npm run test:frontend",
    "lint": "cd frontend && npm run lint",
    "typecheck": "cd frontend && npm run type-check"
  },
  "keywords": [
    "owasp",
    "llm",
    "security",
    "vulnerability",
    "demonstration"
  ],
  "author": "AImaginationLab",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AImaginationLab/vulnerable-llms.git"
  },
  "bugs": {
    "url": "https://github.com/AImaginationLab/vulnerable-llms/issues"
  },
  "homepage": "https://github.com/AImaginationLab/vulnerable-llms#readme",
  "devDependencies": {
    "tsx": "^4.19.4",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
